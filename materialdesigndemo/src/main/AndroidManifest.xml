<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.yangjw.materialdesigndemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CoordinatorActivity" />
        <activity android:name=".AppBarActivity" />
        <activity android:name=".CollapsingToolBarActivity" />
        <activity android:name=".BehaviorActivity" />
        <activity android:name=".SnackActivity" />
        <activity android:name=".FloatingActionButtonActivity" />
        <activity android:name=".AllActivity" />
        <activity android:name=".InflaterActivity" />
        <activity android:name=".ScrollListActivity"></activity>
    </application>

</manifest>
